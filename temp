                "command": "cd /home/kreedlab/affine-mcp-server && node -e \"\nconst { connectWorkspaceSocket, joinWorkspace, loadDoc } = require('./dist/ws.js');\nconst { loadConfig } = require('./dist/config.js');\nconst { loginWithPassword } = require('./dist/auth.js');\nconst Y = require('yjs');\n\n(async () => {\n  const config = loadConfig();\n  const endpoint = config.graphqlEndpoint;\n  const wsUrl = endpoint.replace(/^http/, 'ws').replace(/\\/graphql$/, '');\n  \n  let headers = {};\n  if (config.cookie) headers = { cookie: config.cookie };\n  else if (config.email && config.password) {\n    const cookie = await loginWithPassword(endpoint, config.email, config.password);\n    headers = { cookie };\n  }\n  \n  const socket = await connectWorkspaceSocket(wsUrl, headers);\n  await joinWorkspace(socket, config.workspaceId);\n  \n  const snapshot = await loadDoc(socket, config.workspaceId, 'dbnRM6yAyP');\n  const doc = new Y.Doc();\n  Y.applyUpdate(doc, Buffer.from(snapshot.missing, 'base64'));\n  \n  const blocks = doc.getMap('blocks');\n  \n  // Find all table blocks\n  blocks.forEach((block, id) => {\n    if (block instanceof Y.Map && block.get('sys:flavour') === 'affine:table') {\n      console.log('=== TABLE BLOCK:', id, '===');\n      console.log('parent:', block.get('sys:parent'));\n      \n      const rows = block.get('prop:rows');\n      console.log('rows type:', rows?.constructor?.name);\n      if (rows instanceof Y.Map) {\n        console.log('rows keys:', Array.from(rows.keys()));\n        rows.forEach((v, k) => {\n          console.log('  row', k, ':', v instanceof Y.Map ? 'Y.Map ->' : typeof v, JSON.stringify(v instanceof Y.Map ? v.toJSON() : v));\n        });\n      } else {\n        console.log('rows value:', JSON.stringify(rows));\n      }\n      \n      const cols = block.get('prop:columns');\n      console.log('columns type:', cols?.constructor?.name);\n      if (cols instanceof Y.Map) {\n        console.log('cols keys:', Array.from(cols.keys()));\n        cols.forEach((v, k) => {\n          console.log('  col', k, ':', v instanceof Y.Map ? 'Y.Map ->' : typeof v, JSON.stringify(v instanceof Y.Map ? v.toJSON() : v));\n        });\n      } else {\n        console.log('columns value:', JSON.stringify(cols));\n      }\n      \n      const cells = block.get('prop:cells');\n      console.log('cells type:', cells?.constructor?.name);\n      if (cells instanceof Y.Map) {\n        console.log('cells keys:', Array.from(cells.keys()));\n        cells.forEach((v, k) => {\n          if (v instanceof Y.Map) {\n            const text = v.get('text');\n            console.log('  cell', k, ': Y.Map -> text type:', text?.constructor?.name, 'content:', text?.toString());\n          } else {\n            console.log('  cell', k, ':', typeof v, JSON.stringify(v));\n          }\n        });\n      } else {\n        console.log('cells value:', typeof cells, cells);\n      }\n      console.log('');\n    }\n  });\n  \n  socket.disconnect();\n  process.exit(0);\n})();\n\" 2>&1",
