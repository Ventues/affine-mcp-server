umns.col2.order', 'a01');\n  \n  // Flat format cells\n  const t1 = new Y.Text(); t1.insert(0, 'Hello');\n  block.set('prop:cells.row1:col1.text', t1);\n  const t2 = new Y.Text(); t2.insert(0, 'World');\n  block.set('prop:cells.row1:col2.text', t2);\n  const t3 = new Y.Text(); t3.insert(0, 'Foo');\n  block.set('prop:cells.row2:col1.text', t3);\n  const t4 = new Y.Text(); t4.insert(0, 'Bar');\n  block.set('prop:cells.row2:col2.text', t4);\n  \n  block.set('prop:comments', undefined);\n  block.set('prop:textAlign', undefined);\n  \n  blocks.set(blockId, block);\n  // Insert after the heading\n  const headingIdx = noteChildren.toArray().indexOf('4yc83FAGfH');\n  noteChildren.insert(headingIdx + 1, [blockId]);\n  \n  const delta = Y.encodeStateAsUpdate(doc, prevSV);\n  await pushDocUpdate(socket, wid, 'dbnRM6yAyP', Buffer.from(delta).toString('base64'));\n  \n  console.log('Done! Check Table Test doc in AFFiNE UI.');\n  socket.disconnect();\n  process.exit(0);\n})();\n\" 2>&1",
